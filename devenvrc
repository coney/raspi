export RASPI_LINUX_PATH=$RASPI_BASE/linux
export RASPI_FIRMWARE_PATH=$RASPI_BASE/firmwire

# update cross compiler path
export RASPI_CROSS_PREFIX=arm-linux-gnueabihf-
